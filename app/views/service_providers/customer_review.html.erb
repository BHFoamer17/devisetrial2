<body>
  <div class="container">
    <div class="row">
      <div class="col-md-4">
        <h4>Company Name</h4>
        <p> <%= @service_provider.company_name %></p>
      </div>
    </div>
  </div>

  <nav class="navbar navbar-default" role="navigation">
    <div class="container">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-  collapse-1">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a href="/" class="navbar-brand">SP Navigation</a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
          <li><a href="/service_providers/<%= @service_provider.id %>/service_provider_project">Projects</a></li>
          <li><a href="/service_providers/<%= @service_provider.id %>/customer_review">Reviews</a></li>
          <li><a href="/service_providers/books">Books</a></li>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div>
  </nav>

  <!-- REVIEWS -->

  <div class="container">
    <h2>Reviews for <%= @service_provider.company_name %></h2>
      <div class="row">
        <% @service_provider.reviews.each do |review| %>
          <div class="col-sm-8">
            <div class="thumbnail">
              <h4 class="thumbnail-title"></h4>
              <div class="thumbnail-body">
                <dl>
                  <dd>User Name</dd>

                  <dt>Project Date:</dt>
                  <dd><%= review.try(:month_completed) %><%= review.try(:year_completed) %></dd>

                  <dt>Project Cost:</dt>
                  <dd><%= review.cost %></dd>

                  <dt>Review:</dt>
                  <dd><%= review.review %></dd>
                </dl>
              </div>
            </div>
          </div>
        <% end %>
      </div>
  </div>
</body>


